{
  "name": "tpl-lib-ts-rs",
  "version": "1.0.0",
  "description": "typescript library template with rspack",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "keywords": [
    "library",
    "typescript",
    "template",
    "rspack"
  ],
  "author": "enforcer.a06@gmail.com",
  "license": "MIT",
  "packageManager": "pnpm@9.13.0",
  "scripts": {
    "prepare": "husky",
    "dev": "cross-env NODE_ENV=development rspack serve --config ./build/dev.mjs",
    "build": "cross-env NODE_ENV=production rspack build --config ./build/prod.mjs",
    "docs": "dumi dev",
    "build:docs": "dumi build",
    "preview:docs": "anywhere -d ./docs_dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "demos/**/*.{ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ],
    "src/**/*.{css,less}": [
      "prettier --write"
    ],
    "demos/**/*.{css,less}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@ant-design/icons": "5.5.1",
    "@commitlint/cli": "19.6.0",
    "@commitlint/config-conventional": "19.6.0",
    "@eslint/compat": "1.2.3",
    "@eslint/js": "9.12.0",
    "@module-federation/enhanced": "0.7.6",
    "@npmcli/package-json": "6.1.0",
    "@rsdoctor/rspack-plugin": "0.4.9",
    "@rspack/cli": "1.1.1",
    "@rspack/core": "1.1.1",
    "@rspack/plugin-react-refresh": "1.0.0",
    "@types/eslint__js": "8.42.3",
    "@types/node": "22.10.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "antd": "4.24.16",
    "anywhere": "1.6.0",
    "bundle-declarations-webpack-plugin": "5.1.1",
    "cross-env": "7.0.3",
    "dumi": "2.4.14",
    "eslint": "9.12.0",
    "eslint-plugin-react": "7.37.1",
    "eslint-plugin-react-hooks": "5.0.0",
    "glob": "11.0.0",
    "globals": "15.12.0",
    "husky": "9.1.7",
    "less-loader": "12.2.0",
    "lint-staged": "15.2.10",
    "prettier": "3.3.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-refresh": "0.14.2",
    "react-router-dom": "6.28.0",
    "typescript": "5.7.2",
    "typescript-eslint": "8.16.0",
    "typescript-plugin-css-modules": "5.1.0",
    "webpack-bundle-analyzer": "4.10.2",
    "webpack-merge": "6.0.1",
    "webpack-node-externals": "3.0.0"
  },
  "dependencies": {
    "core-js": "3.39.0"
  }
}
